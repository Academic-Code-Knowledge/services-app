<%= simple_form_for([:admin, @service], :remote => true) do |f| %>
  <%= f.input :name %>
  <%= f.input :description  %>
  <%= f.input :amount %>
  <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
  <button type="submit" class="btn btn-primary">Grabar</button>
<% end %>
